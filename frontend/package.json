{
  "name": "tsflow",
  "version": "1.0.0",
  "description": "Tailscale Network Flow Visualizer - A modern web application for visualizing and analyzing network traffic flows within Tailscale networks",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react-window": "^1.8.8",
    "@xyflow/react": "^12.8.4",
    "clsx": "^2.1.0",
    "d3": "^7.9.0",
    "date-fns": "^2.29.3",
    "elkjs": "^0.10.0",
    "lucide-react": "^0.323.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^3.0.5",
    "react-router-dom": "^6.8.1",
    "react-window": "^2.0.0",
    "swr": "^2.2.4",
    "use-debounce": "^10.0.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.15.30",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
